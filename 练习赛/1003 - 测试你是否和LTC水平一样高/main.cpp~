#include <set> 
#include <map> 
#include <list> 
#include <cmath> 
#include <ctime> 
#include <deque> 
#include <queue> 
#include <stack> 
#include <cctype> 
#include <cstdio> 
#include <string> 
#include <vector> 
#include <cassert> 
#include <cstdlib> 
#include <cstring> 
#include <sstream> 
#include <iostream> 
#include <algorithm> 

using namespace std; 

#define _ISM_DEBUG_ 

#define MAX_SIZE 11000

void cal_LTC (int num)
{
	for (int i = 1; i*i <= MAX_SIZE; i++)
	{
		for (int j = i; i*i+j*j <= MAX_SIZE; j++)
		{
			for (int k = j; i*i+j*j+k*k <= MAX_SIZE; k++)
			{
				if (num == i*i+j*j+k*k)
				{
					cout << i << " " << j << " " << k << endl;
				}
			}
		}
	}
}

int main () 
{ 
	int value;
	while (cin >> value)
	{
		cal_LTC (value);
	}
	return 0; 
} 
