import java.util.Scanner;


public class Main
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		String str = null;
		str = scanner.next();
		
		while ('0' != str.charAt(0))
		{
			str = scanner.next();
		}
		
	}

}
