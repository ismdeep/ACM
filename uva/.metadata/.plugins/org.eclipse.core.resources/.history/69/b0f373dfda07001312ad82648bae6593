import java.util.Scanner;


public class Main
{
	public static void main(String[] args)
	{
		Scanner scanner = new Scanner(System.in);
		
		int t;
		
		t = scanner.nextInt();
		
		
		for (int case_index = 1; case_index <= t; case_index++)
		{
			System.out.print("Case " + case_index + ": ");
		}
		
		
	}
}
